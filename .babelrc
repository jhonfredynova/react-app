{
  "presets": [
    "@babel/preset-env",
    "@babel/preset-react"
  ],
  "plugins": [
    "@loadable/babel-plugin", 
    "syntax-dynamic-import",
    ["styled-jsx/babel", { 
      "plugins": [
        ["styled-jsx-plugin-sass", { "sassOptions": { "includePaths": ["./node_modules"], "precision": 2 } }]
      ] 
    }]
  ]
}
